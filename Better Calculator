num1 = float(input('Enter first number: '))
op = input('Enter operator: ')
num2 = float(input('Enter second number: '))

if op == '+' or 'add':
    print(num1 + num2)
elif op == '-' or 'subtract':
    print(num1 - num2)
elif op == 'divison' or '/':
    print(num1 / num2)
elif op == 'multiplication' or '*' or 'multiply':
    print(num1 * num2)
else:
    print('Invalid Input')